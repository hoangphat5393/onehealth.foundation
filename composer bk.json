{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0.13",
        "amrshawky/laravel-currency": "^6.0",
        "carandclassic/talkjs": "^1.0",
        "ckfinder/ckfinder-laravel-package": "^3.5.3",
        "codezero/laravel-localized-routes": "^3.1",
        "diglactic/laravel-breadcrumbs": "^8.1",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "gornymedia/laravel-shortcodes": "^1.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "harimayco/laravel-menu": "^1.4",
        "illuminate/events": "^8.0",
        "illuminate/session": "^8.0",
        "illuminate/support": "^8.0",
        "intervention/image": "^2.4",
        "ixudra/curl": "6.*",
        "jenssegers/agent": "^2.6",
        "laravel/cashier": "^12.9",
        "laravel/framework": "^8.75",
        "laravel/passport": "10.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.1",
        "lcobucci/jwt": "^3.3.3",
        "maatwebsite/excel": "^3.1",
        "olimortimer/laravelshoppingcart": "^6.0",
        "paypal/rest-api-sdk-php": "^1.14",
        "pgrimaud/instagram-user-feed": "^6.3",
        "phpviet/laravel-omnipay": "^1.1",
        "shipengine/shipengine": "^1.0",
        "stevebauman/location": "^6.6",
        "symfony/symfony": "5.4.x",
        "twilio/sdk": "^7.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.13",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": ["app/Libraries/system.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": ["Illuminate\\Foundation\\ComposerScripts::postAutoloadDump", "@php artisan package:discover --ansi"],
        "post-root-package-install": ["@php -r \"file_exists('.env') || copy('.env.example', '.env');\""],
        "post-create-project-cmd": ["@php artisan key:generate --ansi"]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "0.0.0/composer-include-files": true,
            "php-http/discovery": true,
            "codezero/composer-preload-files": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
